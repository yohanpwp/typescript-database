{
    "version": 2,
    "builds": [
        {
            "src": "app.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.ts"
        }
    ],
    "rewrites": [{
        "source": "/api/(.*)",
        "destination": "/api"
    }]
}